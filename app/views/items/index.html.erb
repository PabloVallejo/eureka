<div class="container">

    <%= link_to 'Registrar objeto', new_item_path, class: 'btn btn-success' %>

    <p id="notice"><%= notice %></p>

    <% if !@items.blank? %>
        <ul class="list-unstyled item-list">
            <% for item in @items %>
                <li><%= item %></li>
            <% end %>
        </ul>
    <% else %>
        <h3>No items</h3>
    <% end %>

    <h1><%= current_user %></h1>


</div>

<fb:login-button scope="public_profile,email" onlogin="checkLoginState();">
</fb:login-button>

<div id="status">
</div>
